{
  "name": "pkg-upgrader",
  "version": "0.0.1",
  "description": "CLI builder to help users upgrade library versions.",
  "license": "MIT",
  "repository": "benmonro/pkg-upgrader",
  "main": "lib/index.js",
  "contributors": [
    {
      "name": "Jeroen Engels",
      "email": "jfm.engels@gmail.com",
      "url": "github.com/jfmengels"
    }
  ],
  "author": {
    "name": "Ben Monro",
    "email": "ben.monro@gmail.com",
    "url": "github.com/benmonro"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "dir-babel --src-dir src --out-dir lib --copy-files",
    "std-version": "standard-version -m \"chore(release): version %s build ${CIRCLE_BUILD_NUM} [ci skip]\"",
    "test": "jest"
  },
  "files": [
    "src/index.js",
    "lib",
    "!*.test.js"
  ],
  "keywords": [
    "codemod",
    "update",
    "upgrade",
    "jscodeshift",
    "transform",
    "npm"
  ],
  "dependencies": {
    "bluebird": "^3.4.0",
    "globby": "^4.1.0",
    "inquirer": "^1.0.2",
    "is-git-clean": "^1.0.0",
    "jscodeshift": "^0.5.0",
    "lodash.assign": "^4.0.9",
    "lodash.findindex": "^4.4.0",
    "lodash.flatten": "^4.2.0",
    "meow": "^3.7.0",
    "semver": "^5.1.0",
    "update-notifier": "^0.7.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "dir-babel": "^1.0.14",
    "jest": "^22.4.2",
    "standard-version": "^4.3.0"
  }
}
